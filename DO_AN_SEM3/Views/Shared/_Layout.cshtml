
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title></title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="~/Content/css/adminlte.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/css/all.min.css">
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <link rel="stylesheet" href="~/Content/css/tempusdominus-bootstrap-4.min.css">
    <link rel="stylesheet" href="~/Content/css/icheck-bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/css/jqvmap.min.css">
    <link rel="stylesheet" href="~/Content/css/adminlte.min.css">
    <link rel="stylesheet" href="~/Content/css/OverlayScrollbars.min.css">
    <link rel="stylesheet" href="~/Content/css/daterangepicker.css">
    <link rel="stylesheet" href="~/Content/css/summernote-bs4.css">
    <link href="~/Content/css/common/style.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="https://cdn.datatables.net/1.10.22/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.2/jquery-confirm.min.css">
    @RenderSection("css", false)
</head>
<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">
        <nav class="main-header navbar navbar-expand navbar-white navbar-light" style="background-color: #ffc107">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#"><i class="fa fa-bars" aria-hidden="true"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="/" class="nav-link text text-white">Home</a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="#" class="nav-link text text-white">Contact</a>
                </li>
            </ul>
            <ul class="navbar-nav ml-auto">
                <li class="nav-item dropdown">
                    <a class="nav-link" data-toggle="dropdown" href="#">
                        @if (DO_AN_SEM3.Models.Common.InforSession.SessionName != null)
                        {
                            <samp style="font-weight: bold; font-style: initial; font-family: initial; color: red">Wellcome to @DO_AN_SEM3.Models.Common.InforSession.SessionName</samp>
                        }
                    </a>
                    <div class="dropdown-menu dropdown-menu-lg dropdown-menu-right">
                        <div class="dropdown-divider"></div>
                        <a href="/Account/Logout" class="dropdown-item">
                            <i class="fa fa-sign-out mr-2" aria-hidden="true"></i> Đăng xuất
                        </a>

                    </div>
                </li>
                <li class="nav-item">
                    <a class="nav-link" data-widget="control-sidebar" data-slide="true" href="#">
                        <i class="fa fa-bars" aria-hidden="true"></i>
                    </a>
                </li>
            </ul>
        </nav>
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <p class="text text-white d-flex justify-content-center mt-3" style="font-size: 20px;">Shop AnHua</p>
            <div class="sidebar">
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="@Url.Content("~/Content/Account/huavanan.jpg")" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">
                        <a href="#" class="d-block text">Hứa Văn An</a>
                    </div>
                </div>
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <li class="nav-item has-treeview menu-open">
                            <a href="#" class="nav-link active">
                                <i class="fa fa-tachometer pr-3" aria-hidden="true"></i>
                                <p class="text">
                                    Dashboard
                                </p>
                            </a>
                        </li>
                        @{
                            Html.RenderAction("_Menu", "Categories");
                        }
                    </ul>
                </nav>
            </div>
        </aside>
        <div class="content-wrapper">
            <section class="content">
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </section>
        </div>
        <footer class="main-footer">
            <div class="d-flex justify-content-end">
                <p class="text">Đồ án sem 3: <span class="text-danger">Hứa Văn An</span></p>
            </div>
        </footer>
        <aside class="control-sidebar control-sidebar-dark">
        </aside>
    </div>
    <script src="~/Content/js/jquery.min.js"></script>
    <script src="~/Content/js/jquery-ui.min.js"></script>
    <script>
        $.widget.bridge('uibutton', $.ui.button)
    </script>
    <script src="~/Content/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/js/sparkline.js"></script>
    <script src="~/Content/js/jquery.vmap.min.js"></script>
    <script src="~/Content/js/jquery.vmap.usa.js"></script>
    <script src="~/Content/js/jquery.knob.min.js"></script>
    <script src="~/Content/js/moment.min.js"></script>
    <script src="~/Content/js/daterangepicker.js"></script>
    <script src="~/Content/js/tempusdominus-bootstrap-4.min.js"></script>
    <script src="~/Content/js/summernote-bs4.min.js"></script>
    <script src="~/Content/js/jquery.overlayScrollbars.min.js"></script>
    <script src="~/Content/js/adminlte.js"></script>
    <script src="~/Content/js/dashboard.js"></script>
    <script src="~/Content/js/demo.js"></script>
    <script src="https://code.jquery.com/jquery-3.5.1.js"></script>
    <script src=" https://cdn.datatables.net/1.10.22/js/jquery.dataTables.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.2/jquery-confirm.min.js"></script>
    <script src="~/Content/js/notify.min.js"></script>
    @RenderSection("scripts", false);
</body>
</html>
